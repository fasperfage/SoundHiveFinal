<AnchorPane id="AnchorPane" prefHeight="109.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.AddPlaylistViewController">
   <children>
      <JFXTextField fx:id="newPlaylistTextField" focusColor="#868686" labelFloat="true" layoutX="24.0" layoutY="23.0" maxWidth="419.0" minWidth="388.0" prefHeight="31.0" prefWidth="388.0" promptText="New Playlist Name">
